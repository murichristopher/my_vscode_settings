{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 500,
  "liveServer.settings.donotShowInfoMsg": true,
  "tabnine.experimentalAutoImports": true,
  "workbench.list.automaticKeyboardNavigation": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "workbench.iconTheme": "material-icon-theme",
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "editor.codeLens": true,
  "editor.linkedEditing": true,
  "liveServer.settings.donotVerifyTags": true,
  "editor.fontSize": 20,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.requireConfig,": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "diffEditor.renderSideBySide": true,
  "editor.formatOnSave": false,
  "html.autoClosingTags": true,
  "editor.autoIndent": "advanced",
  "editor.colorDecorators": true,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira Code, 'Courier New', monospace",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.enabled": true,
  "indentRainbow.colorOnWhiteSpaceOnly": false,
  "editor.insertSpaces": true,
  "indentRainbow.colors": [
    "rgba(255,255,255,0.07)",
    "rgba(127,255,244,0.07)",
    "rgba(255,127,255,0.07)",
    "rgba(79,236,236,0.07)"
  ],
  "editor.inlineSuggest.enabled": true,
  "ruby.useBundler": false, //run non-lint commands with bundle exec
  "ruby.useLanguageServer": true, // use the internal language server (see below)
  "ruby.lint": {
    "rubocop": {
      "useBundler": false // enable rubocop via bundler
    },
    "reek": {
      "useBundler": false // enable reek via bundler
    }
  },
  "ruby.format": "rubocop",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.defaultFormatter": "rebornix.ruby"
  },
  "editor.minimap.enabled": false,
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "javascriptreact": true,
    "html": true,
    "typescript": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "zenMode.silentNotifications": false,
  "terminal.integrated.fontSize": 19,
  "editor.stickyScroll.enabled": true, // use rubocop for formatting,
  "explorer.autoReveal": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/css"
    }
  ],
  "liveSassCompile.settings.showOutputWindowOn": "None",
  "workbench.panel.defaultLocation": "right",
  "terminal.integrated.localEchoEnabled": "off",
  "terminal.integrated.allowWorkspaceConfiguration": true,
  "terminal.integrated.env.linux": {
    "VSC": "VSCODE!"
  },
  "editor.lineDecorationsWidth": 0,
  "editor.glyphMargin": true,
  "errorLens.enabledDiagnosticLevels": ["info", "error"],
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "security.workspace.trust.untrustedFiles": "open",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.saturation": 1,
  "material-icon-theme.opacity": 1,
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.colorTheme": "Evondev Dracula Darker Contrast",
  "window.menuBarVisibility": "toggle",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideTabs": false,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true
  },
  "markdown.preview.fontSize": 16,
  "diffEditor.ignoreTrimWhitespace": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.parameterHints.enabled": false,
  "files.associations": {
    "*.ts": "typescript",
    "*.erb": "erb"
  },
  "[erb]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify"
  },
  "terminal.integrated.tabs.enabled": false
}
